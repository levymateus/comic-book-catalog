{
	"env": {
		"browser": true,
		"jest": true,
		"es6": true
	},
	"parser": "@typescript-eslint/parser",
	"extends": [
		"airbnb"
	],
	"plugins": [
		"import",
		"jsx-a11y",
		"react"
	],
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [
					".js",
					".jsx",
					".ts",
					".tsx"
				],
				"paths": [
					"src"
				]
			}
		}
	},
	"rules": {
		"no-use-before-define": 0,
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".tsx"
				]
			}
		],
		"react/jsx-one-expression-per-line": "off",
		"react/prop-types": "off",
		"import/prefer-default-export": "off",
		"import/extensions": [
			"error",
			{
				"ts": "never"
			}
		]
	}
}